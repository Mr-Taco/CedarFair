.btn
    background:linear-gradient(to bottom, #3ab7e9 0%, #00a1e1 100%)
    padding:12px 25px 14px
    fontFamily(GothamBlack)
    color:white
    border-radius:50px
    position:relative
    box-sizing:border-box
    box-shadow:0px 2px 5px 0px rgba(0,0,0,.4)
    cursor:pointer
    display:block
    text-align:center
    margin:0 auto
    transition:color .3s
    text-decoration:none
    vertical-align:top
    transition: transform .35s ease-in-out, color .35s ease-in-out, box-shadow .1s, color .3s, transform .1s


    &:hover, &.hover
        /*color: #e8e8e8*/        
        box-shadow:0 1px 1px 0 rgba(0,0,0,0.4)
        transform: translate(0px, 2px)        
        transition: box-shadow .1s, color .3s, transform .1s     
        
        &:after
            opacity: 0
        
    &.red
        background:#c62727
        
    &.gray
        background:#b3b3b3
        

    
    &:after
        position:absolute
        top:0px
        left:0px
        right:0px
        bottom:0px  
        border:1px solid #b3ebfa
        border-radius:50px
        content:""
        padding:15px
        box-shadow:inset 0px -1px 3px 0px rgba(0,0,0,.2), inset 0px 1px 3px 0px rgba(255,255,255,.2)
        transition: opacity .3s

    @media (max-width:640px)
        height: 50px
        line-height: 1.4em 
        &:after
            top:2px
            left:2px
            right:2px
            bottom:2px
            box-shadow:inset 0px -1px 3px 0px rgba(0,0,0,.5), inset 0px 1px 3px 0px rgba(255,255,255,.5)
            
            




