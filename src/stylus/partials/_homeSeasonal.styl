#seasonal
        
    max-width: 100%
    position: relative
    top: -115px
    overflow: hidden
    height: 100%

    .content, .arms
        position: absolute
        width: 100%
        height: 100%
        top: 0
        background-size: auto 100%
        background-position: bottom center
        background-repeat no-repeat

    .content
        top: 845px
        height: auto
        z-index: 9999999999999999
        
        @media(max-width: 767px)
            top: 10px
            padding-bottom: 40px

    .dirtForArms
        position: relative
        /*border: 1px solid blue*/
        width: 100%
        max-width: 1050px
        height: 100%
        margin: 0 auto
        z-index: 1
        background: url("../images/seasonal/haunt-arm-left-dirt.png") -3% 110% no-repeat,
                    url("../images/seasonal/haunt-arm-right-dirt.png") 98% 108.5% no-repeat
        
    .arms
        max-width: 1050px
        margin: 0 auto
        left: 0
        right: 0

        &.left
            background: url("../images/seasonal/haunt-arm-left.png") left 140px no-repeat
        &.right
            background: url("../images/seasonal/haunt-arm-right.png") 105% 135% no-repeat 


        top: 0px
        
    .particle-sprite-container
        position: absolute
        height: 350px
        width: 100%
        max-width: 1070px
        top: 825px
        left: 50%
        transform: translateX(-50%)
        z-index: 999999999999
        
    .particle-sprite-relative
        width: 100%
        height: 100%
        position: relative
        z-index: 999999999999
        
    .particle-sprite
