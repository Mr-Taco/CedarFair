    .overlay
        height: 660px
        width: 100%
        /*border: 1px solid white*/
        position: absolute
        left: 0
        right: 0
        top: 500px
        background-size: auto 100%
        background-position: bottom center
        background-repeat no-repeat

        .haunt-text-container
            height: 100%
            width: 100%
            max-width: 900px
            margin: 0 auto
            /*border: 1px solid yellow*/

            div
                width: auto
                text-align: center
                position: relative

                &.halloween-text
                    height: auto
                    margin-bottom: .75%

                &.haunt-text
                    div
                        margin: 0 -1.75%
                        
                    .blood
                        opacity: 0

                &.h
                    width: auto
                    height: auto
                    opacity: 0
                    display: inline-block
                &.a
                    width: auto
                    height: auto
                    opacity: 0
                    display: inline-block
                &.u
                    width: auto
                    height: auto
                    opacity: 0
                    display: inline-block
                &.n
                    width: auto
                    height: auto
                    opacity: 0
                    display: inline-block
                &.t
                    width: auto
                    height: auto
                    opacity: 0
                    display: inline-block
                &.fiftyfive
                    height: auto
                    margin-top: -2%
                &.sightofblood
                    height: auto
                    
                    img
                        width: 100%
                &.waitforhaunt
                    height: auto

                .blood
                    position: absolute
                    top: 0
                    left: 50%
                    transform: translateX(-50%)
                    opacity: 1


        .haunt-blood-container
            position: absolute
            top: 0
            left: 0
            right: 0
            height: 100%
            width: 100%
            max-width: 900px
            margin: 0 auto
            /*border: 1px solid yellow*/
            background: url('../images/seasonal/haunt-text-halloween-blood.png') 293px 0px no-repeat,
            url('../images/seasonal/haunt-text-h-blood.png') 110px 60px no-repeat,
            url('../images/seasonal/haunt-text-a-blood.png') 250px 60px no-repeat,
            url('../images/seasonal/haunt-text-u-blood.png') 390px 60px no-repeat,
            url('../images/seasonal/haunt-text-n-blood.png') 530px 60px no-repeat,
            url('../images/seasonal/haunt-text-t-blood.png') 670px 60px no-repeat,
            url('../images/seasonal/haunt-text-55percent-blood.png') 167px 175px no-repeat,
            url('../images/seasonal/haunt-text-sight-blood.png') 10px 250px no-repeat

    #seasonal-video-gallery
        .container
            max-width:640px
            margin:40px auto 0               

            .video-button .play-icon
                thumbnail(false, 0, -102%, 100%, 180, 37, 37)

                @media (min-width: 992px)
                    &:hover
                        img
                            transform: scale3d(1.2, 1.2, 1) translate(-2.0em, -2.0em);
                
        .right
            text-align:center
            height:100%
            display:table
            float:left
            width:47%
            margin-left: 3%

        .left
            height:100%
            float:left
            width:47%
            margin-right: 3%

.btn-wrapper
    padding-top: 40px




